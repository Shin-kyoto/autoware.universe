<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="param_path" default="$(find-pkg-share autoware_vad_python)/config/simpl_ego_node.param.yaml"/>
  <arg name="data_path" default="$(find-pkg-share autoware_vad_python)/data"/>

  <arg name="input/ego" default="/localization/kinematic_state"/>
  <arg name="input/image" default="/sensing/camera/image0/image_rect_color"/>
  <arg name="output/objects" default="objects"/>

  <arg name="build_only" default="false"/>

  <node pkg="autoware_vad_python" exec="autoware_vad_python_ego_node" name="vad_ego_node" output="screen">
    <param from="$(var param_path)" allow_substs="true"/>
    <remap from="~/input/ego" to="$(var input/ego)"/>
    <remap from="~/input/image" to="$(var input/)"/>
    <remap from="~/output/objects" to="$(var output/objects)"/>
    <param name="build_only" value="$(var build_only)"/>
  </node>
</launch>
